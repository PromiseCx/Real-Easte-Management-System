A,MAAA;AACA,OAFA;AAGA,KALA;AAMA,IAAA,MANA,kBAMA,KANA,EAMA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,KARA;AASA,IAAA,QATA,oBASA,CATA,EASA;AAAA;;AACA,MAAA,CAAA,CAAA,cAAA;AACA,WAAA,IAAA,CAAA,uBAAA,CAAA,UAAA,GAAA,EAAA,MAAA,EAAA;AACA,YAAA,CAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,KAAA,CAAA,UAAA;;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,2BAAA,EAAA,MAAA;AACA;AACA,OALA;AAMA;AAjBA;AAtBA,CAAA","sourcesContent":["<template>\n  <div>\n    <a-form :form=\"form\" style=\"margin: 20px auto 0;\">\n      <a-row>\n        <a-col :span=\"12\">\n          <a-form-item label=\"所属公司\" :labelCol=\"labelCol\" :wrapperCol=\"wrapperCol\">\n            <a-select\n              @change=\"company\"\n              v-decorator=\"[\n                'company',\n                { rules: [{ required: true, message: '所属公司必须填写', trigger: 'change' }] },\n              ]\"\n            >\n             <a-select-option :key=\"index\" :value=\"item.id+''\" v-for=\"(item, index) in select\">\n                 {{ item.companyFullName }}\n             </a-select-option>\n            </a-select>\n          </a-form-item>\n        </a-col>\n      </a-row>\n      <a-row>\n        <a-col :span=\"12\">\n          <a-form-item label=\"选择住宅\" :labelCol=\"labelCol\" :wrapperCol=\"wrapperCol\">\n            <a-select\n              @change=\"estate\"\n              v-decorator=\"[\n                'estate',\n                { rules: [{ required: true, message: '选择住宅必须填写', trigger: 'change' }] },\n              ]\"\n            >\n                <a-select-option :key=\"index\" :value=\"item.estateCode+''\" v-for=\"(item, index) in estateSelect\">\n                    {{ item.estateName }}\n                </a-select-option>\n            </a-select>\n          </a-form-item>\n        </a-col>\n        <a-col :span=\"12\">\n          <a-form-item label=\"新增楼宇数量\" :labelCol=\"labelCol\" :wrapperCol=\"wrapperCol\">\n            <a-input\n              v-decorator=\"[\n                'addhomecount',\n                { rules: [{ required: true, message: '新增楼宇数量必须填写', trigger: 'blur' }] },\n              ]\"\n            />\n          </a-form-item>\n        </a-col>\n      </a-row>\n      <a-form-item :wrapperCol=\"{span: 19, offset: 5}\">\n        <a-button type=\"primary\" @click=\"nextStep\">下一步</a-button>\n        <a-button style=\"margin-left: 10px;\">Reset</a-button>\n      </a-form-item>\n    </a-form>\n  </div>\n</template>\n\n<script>\nimport { selectCompany, selectEstate } from '@/api/estate'\nconst QS = require('qs')\nexport default {\n    name: 'Step1',\n    data() {\n        return {\n            labelCol: { lg: { span: 6 }, sm: { span: 4 } },\n            wrapperCol: { lg: { span: 16 }, sm: { span: 20 } },\n            form: this.$form.createForm(this),\n            select: [],\n            estateSelect: []\n        }\n    },\n    created() {\n        selectCompany().then(res => {\n            this.select = res.result\n        }).catch(err => {\n            this.$notification['error']({\n                message: '错误',\n                description: err.toString(),\n                duration: 1\n            })\n        })\n    },\n    methods: {\n        company(value) {\n           selectEstate(QS.stringify( { company: value })).then(res => {\n               this.estateSelect = res.result\n           })\n        },\n        estate(value) {\n            console.log(value)\n        },\n        nextStep(e) {\n            e.preventDefault()\n            this.form.validateFieldsAndScroll((err, values) => {\n                if (!err) {\n                    this.$emit('nextStep')\n                    console.log('Received values of form: ', values)\n                }\n            })\n        }\n    }\n}\n</script>\n\n<style lang=\"less\" scoped>\n.step-form-style-desc {\n    padding: 0 56px;\n    color: rgba(0, 0, 0, 0.45);\n\n    h3 {\n        margin: 0 0 12px;\n        color: rgba(0, 0, 0, 0.45);\n        font-size: 16px;\n        line-height: 32px;\n    }\n\n    h4 {\n        margin: 0 0 4px;\n        color: rgba(0, 0, 0, 0.45);\n        font-size: 14px;\n        line-height: 22px;\n    }\n\n    p {\n        margin-top: 0;\n        margin-bottom: 12px;\n        line-height: 22px;\n    }\n}\n.ant-form-item {\n    margin-bottom: 8px;\n}\n</style>\n"],"sourceRoot":"src/views/property/batchAddGuide"}]}